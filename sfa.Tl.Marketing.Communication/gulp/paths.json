{
  "src": {
    "default": "Frontend/Frontend/src/",
    "SCSS": "Frontend/src/scss/*.scss",
    "SCSSWATCH": "Frontend/src/scss/**/*.scss",
    "JS": "Frontend/src/js/*.js",
    "Assets": "Frontend/src/assets/**/*",
    "plyr": "node_modules/plyr/src/sass/**/*.scss"
  },

  "dist": {
    "default": "wwwroot",
    "CSS": "wwwroot/css",
    "SCSS": "wwwroot/css",
    "JS": "wwwroot/js",
    "Assets": "wwwroot/files",
    "Bootstrap": "wwwroot/bootstrap",
    "plyr": "wwwroot/plyr"
  }
}